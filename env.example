# Server Configuration
NODE_ENV=development
PORT=3001

# Database
DATABASE_URL=./roadtime.db

# CORS Configuration (for production, set to your actual domain)
CORS_ORIGIN=http://localhost:3000

# WebSocket Configuration
WS_PORT=3001

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# Analytics
ENABLE_ANALYTICS=true

# Community Features
ENABLE_COMMUNITY_FEATURES=true
COMMUNITY_DATA_RETENTION_DAYS=365

# Performance
ENABLE_COMPRESSION=true
ENABLE_CACHING=true
